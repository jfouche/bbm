{
    "configurations": [{
            "name": "Win64",
            "includePath": [
                "${workspaceFolder}/**",
                "${config:QT_DIR}/include/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            // "compilerPath": "${config:MINGW_DIR}\\bin\\gcc.exe",
            "compilerPath": "cl.exe",
            "cStandard": "c11",
            "cppStandard": "gnu++14",
            // "intelliSenseMode": "clang-x86",
            "intelliSenseMode": "msvc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        },
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include/x86_64-linux-gnu/qt5/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "gcc",
            "cStandard": "c11",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "clang-x86"
        }
    ],
    "version": 4
}